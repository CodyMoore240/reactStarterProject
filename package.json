      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/registerServiceWorker.js",
      "!src/tempPolyfills.js",
      "!src/modules/index.js",
      "!src/store.js",
      "!src/firebaseDB.js",
      "!src/shallowWithStore.js",
      "!src/**/_story.js"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.11",
    "@storybook/addon-backgrounds": "^3.4.11",
